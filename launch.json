    "version": "0.2.0",
    "configurations": [
        {
            "name": "sponge debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/cs144/sponge/build/tests/send_extra",	//对应要调试的程序
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "console": "integratedTerminal",
            //"preLaunchTask": "C/C++: g++-9 build active file",	//注释掉
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}

